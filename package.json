    ]
  },
  "lint-staged": {
    "app/**/*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "index.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && yarn lint-staged"
    }
  },
